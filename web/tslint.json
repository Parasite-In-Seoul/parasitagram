{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-config-prettier"
	],
	"jsRules":{
		"object-literal-sort-keys": false,
		"no-console": false
	},
	"rules": {
		"quotemark": false,
		"interface-over-type-literal": false,
		"member-access": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"jsx-no-lambda": false,
		"no-console": false

	},
	"rulesDirectory": []
}